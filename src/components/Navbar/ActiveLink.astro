---
import { PageLink } from './types';
interface Props extends PageLink {}

const { title, link } = Astro.props;
---

<li>
  <a
    href={link}
    class="block py-2 px-3 text-white bg-primary-700 rounded md:bg-transparent md:text-blue-700 md:p-0 dark:text-white md:dark:text-secondary-300"
    aria-current="page">{title}</a
  >
</li>
